<div id="<%= dom_id parent %>">
  <p>
    <strong>User:</strong>
    <%= parent.user_id %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= parent.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= parent.last_name %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= parent.phone_number %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= parent.email %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= parent.address %>
  </p>

</div>
