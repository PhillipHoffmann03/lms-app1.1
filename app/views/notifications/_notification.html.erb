<div id="<%= dom_id notification %>">
  <p>
    <strong>User:</strong>
    <%= notification.user_id %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= notification.message %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= notification.type %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= notification.status %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= notification.date %>
  </p>

</div>
